{
    "manifest_version": 2,
    "name": "Up2Date",
    "description": "Gives you news up 2 date!",
    "version": "0.5",
    "browser_action": {
        "default_icon": "uicon.png",
        "default_popup": "popup.html"
    },
    "background":
    {
      "scripts": ["app.js"]
    },
    "permissions": [
        "activeTab",
        "http://*/",
        "identity"
    ],
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+O",
          "mac": "Command+Shift+O"
        },
        "description": "Opens popup.html"
      }
    },


    "oauth2": {
    "client_id": "220706775478-mg1tmjrv482b1sv4infabtch891e68us.apps.googleusercontent.com",
    "scopes": [
      "profile"
    ]
  }
}
